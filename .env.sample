# Agent Data Environment Configuration Sample
# Copy this file to .env and fill in the actual values

# ⬛ Google Cloud Configuration
GCP_PROJECT_ID=chatgpt-db-project
GCP_REGION=asia-southeast1
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account-key.json

# ⬛ Qdrant Configuration
QDRANT_URL=https://ba0aa7ef-be87-47b4-96de-7d36ca4527a8.us-east4-0.gcp.cloud.qdrant.io:6333
QDRANT_API_KEY=your-qdrant-api-key-here
QDRANT_COLLECTION_NAME=agent_data_collection

# ⬛ OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_MODEL=gpt-4o-mini

# ⬛ Firestore Configuration
FIRESTORE_PROJECT_ID=chatgpt-db-project
FIRESTORE_DATABASE_ID=(default)

# ⬛ Test Configuration
PYTEST_TIMEOUT=8
BATCH_SIZE=3
RUN_DEFERRED=0
PYTHONDONTWRITEBYTECODE=1

# ⬛ Development Settings
DEBUG=false
LOG_LEVEL=INFO
ENVIRONMENT=development

# ⬛ CI/CD Settings
CI=false
GITHUB_ACTIONS=false

# ⬛ Storage Configuration
GCS_BUCKET_NAME=huyen1974-faiss-index-storage
STORAGE_REGION=asia-southeast1

# ⬛ Authentication
AUTH_ENABLED=true
JWT_SECRET_KEY=your-jwt-secret-key-here

# ⬛ API Configuration
API_HOST=0.0.0.0
API_PORT=8000
API_PREFIX=/api/v1

# ⬛ MCP Configuration
MCP_SERVER_HOST=localhost
MCP_SERVER_PORT=8001
