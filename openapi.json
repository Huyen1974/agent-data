{
  "components": {
    "schemas": {
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "title": "Detail",
            "type": "array"
          }
        },
        "title": "HTTPValidationError",
        "type": "object"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "title": "Location",
            "type": "array"
          },
          "msg": {
            "title": "Message",
            "type": "string"
          },
          "type": {
            "title": "Error Type",
            "type": "string"
          }
        },
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError",
        "type": "object"
      },
      "VectorSearchRequest": {
        "properties": {
          "filter_tag": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "description": "Optional tag to filter results",
            "title": "Filter Tag"
          },
          "query_vector": {
            "description": "Vector to search for (embedding)",
            "items": {
              "type": "number"
            },
            "title": "Query Vector",
            "type": "array"
          },
          "top_k": {
            "anyOf": [
              {
                "type": "integer"
              },
              {
                "type": "null"
              }
            ],
            "default": 5,
            "description": "Number of results to return",
            "title": "Top K"
          }
        },
        "required": [
          "query_vector"
        ],
        "title": "VectorSearchRequest",
        "type": "object"
      }
    }
  },
  "info": {
    "description": "API for vector search using QdrantStore",
    "title": "Vector Search API",
    "version": "0.1.0"
  },
  "openapi": "3.1.0",
  "paths": {
    "/health": {
      "get": {
        "description": "Health check endpoint to verify API is running",
        "operationId": "health_check_health_get",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {}
              }
            },
            "description": "Successful Response"
          }
        },
        "summary": "Health Check"
      }
    },
    "/metrics": {
      "get": {
        "description": "Exposes Prometheus metrics for monitoring API performance including request counts, latency, and status codes.",
        "operationId": "prometheus_metrics_metrics_get",
        "responses": {
          "200": {
            "content": {
              "text/plain": {
                "schema": {
                  "title": "Prometheus Metrics",
                  "type": "string"
                }
              }
            },
            "description": "Metrics data in Prometheus format"
          }
        },
        "summary": "Prometheus Metrics"
      }
    },
    "/search_vector": {
      "post": {
        "description": "Search for vectors in the Qdrant collection that are similar to the query vector.\n\nArgs:\n    request: Vector search request containing query_vector, top_k, and filter_tag\n\nReturns:\n    List of search results with point IDs and payloads",
        "operationId": "search_vector_search_vector_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/VectorSearchRequest"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "additionalProperties": true,
                  "title": "Response Search Vector Search Vector Post",
                  "type": "object"
                }
              }
            },
            "description": "Successful Response"
          },
          "422": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            },
            "description": "Validation Error"
          }
        },
        "summary": "Search Vector"
      }
    }
  }
}
